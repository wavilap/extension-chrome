{
	"manifest_version": 2,
	"name": "Credencial Fdw",
	"version": "1.0",
	"description": "Este es un ejemplo de uso de Google Extension",
	"background": {"page": "background.html"},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"browser_action": {
		"default_title": "Icono de app",
		"default_icon": "images/icon.png"
	},
	"permissions": [
		"http://www.forosdelweb.com/"
	]
}